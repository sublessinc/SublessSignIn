{
  "name": "sublessui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true --proxy-config proxy.conf.json",
    "build": "ng build --configuration dev && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "build-dev": "ng build --configuration dev && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "build-test": "ng build --configuration test && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "build-stage": "ng build --configuration stage && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "build-prod": "ng build --configuration prod && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/core": "~12.2.15",
    "@angular/forms": "~12.2.15",
    "@angular/material": "^12.1.4",
    "@angular/platform-browser": "~12.2.15",
    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "file-saver": "^2.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular/cli": "~12.0.4",
    "@angular/compiler-cli": "~12.0.4",
    "@types/file-saver": "^2.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "typescript": "~4.2.3"
  }
}