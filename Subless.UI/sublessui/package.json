{
  "name": "sublessui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true --proxy-config proxy.conf.json",
    "build": "ng build --configuration dev && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "build-dev": "ng build --configuration dev && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "build-test": "ng build --configuration test && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "build-stage": "ng build --configuration stage && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "build-prod": "ng build --configuration prod && cp -r ./dist/sublessui/* ../../SublessSignIn/wwwroot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "^12.1.4",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^12.2.4",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "file-saver": "^2.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@types/file-saver": "^2.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.5.4"
  }
}